# Database settings
{% for db in django_settings_databases %}{% for alias, cred in db.items() %}
DATABASES["{{alias}}"] = DATABASES["{{alias}}"] or {}
{% for name, value in cred.items() -%}
DATABASES["{{alias}}"]["{{name}}"] = "{{value}}"
{% endfor %}{% endfor %}{% endfor %}

# Cache settings
{% for cache in django_settings_caches %}{% for alias, cred in cache.items() %}
CACHES["{{alias}}"] = CACHES.get("{{alias}}") or {}
{% for name, value in cred.items() -%}
{% if name == 'OPTIONS' %}
CACHES["{{alias}}"]["{{name}}"] = CACHES["{{alias}}"].get("{{name}}") or {}
{% for option_name, option_value in value.items() -%}
CACHES["{{alias}}"]["{{name}}"]["{{option_name}}"] = "{{option_value}}"
{% endfor %}
{% else %}
CACHES["{{alias}}"]["{{name}}"] = "{{value}}"
{% endif %}
{% endfor %}{% endfor %}{% endfor %}

# Environment settings
{% for option in django_settings_additional %}
{{ option }}
{% endfor %}

